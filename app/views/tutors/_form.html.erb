<%= form_for @attraction do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :subject %>
  <%= f.text_field :subject %>
  <br>

  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <br>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>

  <%= f.submit %>

<% end %>
