<%= form_for @tutor do |f| %>

  <%= f.label :name %> <br>
  <%= f.text_field :name %>
  <br>

  <%= f.label :subject %> <br>
  <%= f.text_field :subject %>
  <br>

  <%= f.label :bio %> <br>
  <%= f.text_area :bio %>
  <br>

  <%= f.label :email %> <br>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.submit %>

<% end %>
